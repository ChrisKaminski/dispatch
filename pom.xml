<?xml version="1.0" encoding="UTF-8"?><project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>net.databinder</groupId>
  <artifactId>databinder</artifactId>
  <name>Databinder</name>
  <version>0.8-SNAPSHOT</version>
  <description>A simple bridge from Wicket to Hibernate</description>
  <url>http://databinder.net</url>
  <inceptionYear>2005</inceptionYear>
  <licenses>
    <license>
      <name>GNU Lesser General Public License</name>
      <url>http://www.gnu.org/copyleft/lesser.txt</url>
      <distribution>repo</distribution>
    </license>
  </licenses>
  <scm>
    <connection>scm:svn:svn://localhost/databinder/trunk</connection>
    <developerConnection>scm:svn:svn://localhost/databinder/trunk</developerConnection>
  </scm>
  <organization>
    <name>Nathan Hamblen</name>
  </organization>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-release-plugin</artifactId>
        <configuration>
          <tagBase>svn://localhost/databinder/</tagBase>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-source-plugin</artifactId>
        <executions>
          <execution>
            <id>attach-sources</id>
            <goals>
              <goal>jar</goal>
            </goals>
          </execution>
        </executions>
        <inherited>true</inherited>
      </plugin>
    </plugins>
  </build>
  <repositories>
    <repository>
      <id>wicket</id>
      <name>Wicket repository</name>
      <url>http://wicket.sourceforge.net/maven2</url>
    </repository>
  </repositories>
  <dependencies>
    <dependency>
      <groupId>javax.servlet</groupId>
      <artifactId>servlet-api</artifactId>
      <version>2.3</version>
    </dependency>
    <dependency>
      <groupId>org.hibernate</groupId>
      <artifactId>hibernate-annotations</artifactId>
      <version>3.2.0.cr2</version>
    </dependency>
    <dependency>
      <groupId>wicket</groupId>
      <artifactId>wicket-auth-roles</artifactId>
      <version>1.2.2</version>
      <exclusions>
        <exclusion>
          <artifactId>log4j</artifactId>
          <groupId>log4j</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>c3p0</groupId>
      <artifactId>c3p0</artifactId>
      <version>0.9.0.4</version>
    </dependency>
  </dependencies>
  <reporting>
    <plugins>
      <plugin>
        <artifactId>maven-project-info-reports-plugin</artifactId>
        <reportSets>
          <reportSet>
            <reports>
              <report>dependencies</report>
              <report>license</report>
            </reports>
          </reportSet>
        </reportSets>
      </plugin>
      <plugin>
        <artifactId>maven-site-plugin</artifactId>
        <configuration>
          <inputEncoding>UTF-8</inputEncoding>
          <outputEncoding>UTF-8</outputEncoding>
        </configuration>
      </plugin>
    </plugins>
  </reporting>
  <distributionManagement>
    <repository>
      <id>databinder</id>
      <name>Databinder repository</name>
      <url>file:///var/dbwww/maven</url>
    </repository>
    <snapshotRepository>
      <id>databinder-snapshots</id>
      <name>Databinder snapshot repository</name>
      <url>file:///var/dbwww/snapshot</url>
    </snapshotRepository>
    <site>
      <id>website</id>
      <url>file:///var/dbwww</url>
    </site>
  </distributionManagement>
</project>
